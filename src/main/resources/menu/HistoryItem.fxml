<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox fx:id="historyItemBox" alignment="CENTER_LEFT" maxHeight="35.0" maxWidth="300.0" minHeight="35.0" minWidth="250.0" spacing="10.0" stylesheets="@../styles/Adreana.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.yohann.manager.controller.menu.MenuItem">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
       <ImageView fx:id="menuIcon" fitHeight="32.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" />
       <Label fx:id="menuName" prefHeight="18.0" prefWidth="190.0" textFill="WHITE" />
   </children>
</HBox>

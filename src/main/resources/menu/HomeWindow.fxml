<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="homeWindowSP" stylesheets="@../styles/Adreana.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.yohann.bsim.controller.HomeWindowController">
	<children>
        <SplitPane fx:id="splitPane">
			<items>
                <VBox fx:id="tabDashboardBox" alignment="TOP_CENTER" SplitPane.resizableWithParent="false">
                    <children>
                        <StackPane fx:id="dashboardContainer" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
			</items>
		</SplitPane>
	</children>
</StackPane>
